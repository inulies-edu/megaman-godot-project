[application]

config/name="Rockman"
run/main_scene="res://Scenes/Stages/Fase0.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://Scripts/Global/Global.gd"
TransitionScreen="*res://Scenes/Misc/TransitionScreen.tscn"
PlayerSfx="*res://Sfx/PlayerSfx.tscn"
player="*res://Scenes/Global/Player.tscn"
gd="*res://addons/godot-action/utils_gd_action.tscn"
sp="*res://addons/awesome_splash/utils_splash.gd"

[display]

window/size/width=576
window/size/height=324
window/size/test_width=1095
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/RandomAudioNodePlayer/plugin.cfg", "res://addons/awesome_splash/plugin.cfg", "res://addons/godot-action/plugin.cfg", "res://addons/randomAudioStreamPlayer/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
